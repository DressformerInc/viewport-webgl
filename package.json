{
    "name": "viewport-webgl",
    "version": "0.0.30",
    "description": "WebGL view port for plugin dressformer",
    "main": "",
    "dependencies": {},
    "devDependencies": {
        "gulp": "^3.8.6",
        "gulp-bump": "^0.1.6",
        "gulp-concat": "^2.3.4",
        "gulp-exec": "^1.0.4",
        "gulp-replace": "^0.4.0",
        "gulp-rimraf": "latest",
        "gulp-sourcemaps": "^1.1.0",
        "gulp-browserify": "latest",
        "gulp-rename": "latest",
        "napa": "^0.4.1",
        "q": "^1.0.0",
        "request": "^2.34.0",
        "run-sequence": "^0.3.6",
        "glslify": "latest",
        "browserify-shim": "~3.2.0"
    },
    "browserify-shim": {
        "jquery": "$",
        "three": "global:THREE"
    },
    "browserify": {
        "transform": [
            "glslify",
            "browserify-shim"
        ]
    },
    "scripts": {
        "install": "napa",
        "build-js": "browserify src/js/main.js -t glslify > src/js/viewport-webgl.js -d",
        "watch-js": "watchify src/js/main.js -o dist/viewport-webgl.js -dv",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "napa": {
        "threejs": "mrdoob/three.js",
        "threejs-stats": "mrdoob/stats.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DressformerInc/viewport-webgl.git"
    },
    "author": "Miha-ha",
